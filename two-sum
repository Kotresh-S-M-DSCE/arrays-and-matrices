class Solution {
    public int[] twoSum(int[] nums, int target) {
        int i=0,j=1;
        while(j<nums.length)
        {
            if(nums[i]+nums[j]==target)
            {
                return new int[] {i,j};
            }
            j+=1;
            if(j==nums.length)
            {
                i+=1;
                j=i+1;
            }
        }
        return null;
    }
}